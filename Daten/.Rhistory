library(tidyverse)
library(ggplot2movies)
library(ggbeeswarm)
data <- movies
#Wir nehmen unsere Daten aus dem Movie Dataset heraus
plot.1.beeswarm <- data %>%
#Wir nehmen unsere Spalten für die Bewertung der Filme
select("length", "Romance") %>%
filter(Romance > 0) %>%
drop_na() %>%
sample_n(2000) %>%
select(-Romance) %>%
#Wir ziehen unseren Data Frame lang damit ggplot damit arbeiten kann
pivot_longer(
cols = everything(),
names_to = "Werte",
values_to = "Zahlen"
)
#1.
ggplot(plot.1.beeswarm, aes(x = "" ,y = Zahlen)) +
#Wir verwenden ggbeeswarm damit wir
geom_beeswarm(alpha = 0.9, color = "#2D708EFF", aes(fill = Zahlen)) +
labs(
title = "Jitter/ Beeswarmplot",
subtitle = "Verteilung der Ratings aller Romantischen Filme",
x = "",
y = "Länge (in Minuten)"
) +
theme_minimal(base_size = 12) +
theme(
legend.position = "none",
)
plot.2 <- plot.1.beeswarm %>%
mutate(
Zahlen = Zahlen/60
)
#2.
ggplot(plot.2, aes(x = Zahlen, y = after_stat(density), fill = Zahlen)) +
geom_histogram(binwidth = 1, alpha = 0.9, color = "#2D708EFF", fill = "#2D708EFF", alpha = 0.9) +
scale_fill_viridis_c(option = "mako", end = 0.3, begin = 0.8) +
labs(
title = "Histogramm",
subtitle = "Verteilung der Länge von Romantischen Filmen",
x = "Länge (in Stunden)",
y = "Dichte"
) +
theme_minimal(base_size = 12)
#3.
ggplot(plot.1.beeswarm, aes(x = Zahlen, y = after_stat(density))) +
geom_density(alpha = 0.8, color = "#2D708EFF", fill = "#2D708EFF") +
theme_minimal(base_size = 12)
#4.
ggplot(plot.1.beeswarm, aes(x = "", y = Zahlen)) +
geom_boxplot(alpha = 0.9, color = "#2D708EFF", fill = "#2D708EFF") +
theme_minimal(base_size = 12) +
labs(
title = "Boxplot",
subtitle = "Verteilung der Filmlänge Romantischer Filme "
)
